{
  "compilerOptions": {
    "strictNullChecks": true,
    "module": "commonjs",
    "typeRoots": ["declarations", "node_modules/@types"],
    "declaration": true,
    "lib": ["es2015"],
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
